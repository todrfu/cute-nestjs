{
  "name": "@otdrfu/cute-nestjs",
  "version": "0.0.1",
  "description": "一个轻量级的类 NestJS 框架实现，专注于实现NestJS的核心特性，便于学习、理解NestJS的设计理念和实现原理。",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && rollup -c rollup.config.mjs",
    "dev": "tsc --watch",
    "watch": "rollup -c rollup.config.mjs --watch",
    "test": "jest",
    "clean": "rm -rf dist node_modules"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "nestjs",
    "midway"
  ],
  "dependencies": {
    "fast-glob": "^3.3.3",
    "koa": "^2.15.4",
    "koa-bodyparser": "^4.4.1",
    "koa-router": "^13.0.1",
    "reflect-metadata": "^0.2.2",
    "uuid": "^9.0.1"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^4.3.12",
    "@types/koa-router": "^7.4.8",
    "@types/node": "^20.11.19",
    "rollup": "^4.34.8",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "@types/uuid": "^9.0.7"
  },
  "author": "drfu",
  "license": "MIT"
}